{
  "hosting": {
    "public": "src/public",
    "rewrites":[ 
      {
        "source": "**/**",
        "function": "app-next"
      }
    ]},
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "source": "src/functions"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
